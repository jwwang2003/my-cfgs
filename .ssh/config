# Linux (ubuntu) version

# Personal

# DORM PC (via cpolar reverse proxy)
Host wjw_dorm
	Hostname 1.tcp.cpolar.cn
	Port 21707
	User wjw_0

	ServerAliveInterval 30
	ServerAliveCountMax 5

Host wjw_dorm_wsl
	User wjw
	Hostname 127.0.0.1
	Port 2222		# SSDH port of the WSL instance, for have to enable
	IdentityFile ~/.ssh/id_ed25519
	IdentitiesOnly yes
	ForwardX11 yes
	ForwardX11Trusted yes
	ServerAliveInterval 30
	ServerAliveCountMax 5

	ProxyCommand ssh -T wjw_dorm "cmd.exe /c wsl.exe -d Ubuntu-24.04 bash -lc \"exec nc -q 0 127.0.0.1 2222\""

